r\n  color: #fff;\r\n  margin-bottom: 20px;\r\n  text-shadow: 0px 0px 8px rgba(0, 0, 0, 0.4); /* Add text shadow for added effect*/\r\n background-image: linear-gradient(to right, #007bff, #00c7ff); /* Add gradient background */\r\n -webkit-background-clip: text; /* Apply gradient background as text color */\r\n  color: transparent;\r\n  display: inline;\r\n}\r\n\r\n/*\r\n.hero-title {\r\n  font-size: 3rem;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  color: #fff;\r\n  margin-bottom: 20px;\r\n  text-shadow: 0px 0px 8px rgba(0, 0, 0, 0.4);\r\n  background-image: linear-gradient(to right, #007bff, #00c7ff);\r\n  -webkit-background-clip: text;\r\n  color: transparent;\r\n  display: inline;\r\n  position: relative; /* Add relative positioning for absolute positioning of child elements\r\n} */\r\n\r\n.hero-title::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  bottom: -8px; /* Adjust the value to control the distance of the pop-up effect */\r\n  left: 0;\r\n  width: 100%;\r\n  height: 3px;\r\n  background-color: #00c7ff;\r\n  animation: pop-up 1s ease infinite; /* Add pop-up animation */\r\n  transform-origin: center bottom;\r\n} \r\n\r\n@keyframes pop-up {\r\n  0% {\r\n    transform: scale(1); /* Initial scale */\r\n  }\r\n  50% {\r\n    transform: scale(1.2); /* Scale up to 120% halfway through animation */\r\n  }\r\n  100% {\r\n    transform: scale(1); /* Scale back to 100% at the end of animation */\r\n  }\r\n}\r\n\r\n\r\n\r\n.hero-subtitle {\r\n  font-size: 1.5rem;\r\n  text-align: center;\r\n  color: #fff;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.btn {\r\n  display: inline-block;\r\n  padding: 15px 30px;\r\n  font-size: 1rem;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  color: #fff;\r\n  background-color: #007bff;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.btn:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n/* About section */\r\n.about {\r\n  padding: 100px 0;\r\n}\r\n\r\n.about-title {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  color: #007bff;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.about-text {\r\n  font-size: 1rem;\r\n  text-align: center;\r\n  color: #444;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Events section */\r\n.events {\r\n  padding: 80px 0;\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.events-title {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  color: #007bff;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.events-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\r\n  gap: 30px;\r\n}\r\n\r\n.event-card {\r\n  background-color: #fff;\r\n  border-radius: 4px;\r\n  padding: 20px;\r\n  box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.event-img {\r\n  width: 100%;\r\n  height: 200px;\r\n  object-fit: cover;\r\n  border-radius: 4px;\r\n  margin-bottom: 20px;\r\n}"},"lineCount":null}},"error":null,"hash":"62dd98af4e43a27764377bd5b59614a8","cacheData":{"env":{}}}